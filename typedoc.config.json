{
    "$schema": "https://typedoc.org/schema.json",
    "entryPoints": ["core/types.d.ts"],
    "docsRoot":"./docs",
    "out": "docs/api",
    "tsconfig": "tsconfig.json",
    "plugin": ["typedoc-plugin-vue","typedoc-plugin-markdown","typedoc-vitepress-theme"],
    "readme": "none",
    "excludeExternals": true,
    "entryPointStrategy": "resolve",
    "categorizeByGroup":true
}